{
  "name": "landworks-frontend",
  "author": "enterdao",
  "license": "Apache-2",
  "version": "0.1.0",
  "private": false,
  "scripts": {
    "postinstall": "husky install",
    "clean": "rimraf build/",
    "prebuild": "npm run clean",
    "build": "GENERATE_SOURCEMAP=false node ./scripts/build-non-split.js",
    "build:prod": "react-scripts build",
    "start": "react-scripts start",
    "start:test": "node -r dotenv/config ./node_modules/.bin/react-scripts start dotenv_config_path=.env.test.local",
    "start:prod": "node -r dotenv/config ./node_modules/.bin/react-scripts start dotenv_config_path=.env.production.local",
    "lint": "eslint src/**/*.ts{,x} && tsc --noEmit && stylelint --fix 'src/**/*.scss' --custom-syntax postcss-scss",
    "format": "./node_modules/.bin/prettier --write src/**/*.ts{,x}",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "stylelint-check": "stylelint-config-prettier-check"
  },
  "engines": {
    "node": "> 12.x",
    "npm": "> 6.x"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{json,md,yml}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.scss": [
      "stylelint --cache --fix 'src/**/*.scss' --custom-syntax postcss-scss"
    ]
  },
  "importSort": {
    ".ts, .tsx": {
      "style": "vmv",
      "parser": "typescript"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "resolutions": {
    "react-error-overlay": "6.0.9"
  },
  "dependencies": {
    "@apollo/client": "^3.6.0",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.8.1",
    "@ethersproject/abi": "5.0.13",
    "@ethersproject/providers": "5.4.1",
    "@mui/base": "^5.0.0-alpha.84",
    "@mui/lab": "^5.0.0-alpha.85",
    "@mui/material": "^5.8.3",
    "@mui/system": "^5.4.1",
    "@mui/x-date-pickers": "^5.0.11",
    "@notionhq/client": "^1.0.4",
    "@rooks/use-debounce": "4.10.0",
    "@rooks/use-window-event-listener": "4.10.0",
    "@splitbee/web": "^0.3.0",
    "@typeform/embed-react": "^2.10.0",
    "@web3-react/abstract-connector": "^6.0.7",
    "@web3-react/core": "^6.1.9",
    "@web3-react/injected-connector": "^6.0.7",
    "@web3-react/ledger-connector": "^6.1.9",
    "@web3-react/portis-connector": "^6.1.9",
    "@web3-react/trezor-connector": "^6.1.9",
    "@web3-react/walletconnect-connector": "^6.2.4",
    "@web3-react/walletlink-connector": "^6.2.3",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "bignumber.js": "^9.0.1",
    "classnames": "2.2.6",
    "date-fns": "^2.25.0",
    "ethers": "^5.4.1",
    "graphql": "^15.5.1",
    "identicon.js": "2.3.3",
    "leaflet": "^1.8.0",
    "lodash": "4.17.21",
    "lodash.debounce": "^4.0.8",
    "nanoid": "3.1.22",
    "query-string": "7.0.0",
    "react": "^18.1.0",
    "react-countdown": "^2.3.2",
    "react-device-detect": "2.2.2",
    "react-dom": "^18.1.0",
    "react-leaflet": "^4.0.0",
    "react-loading-skeleton": "^3.0.2",
    "react-popper": "^2.3.0",
    "react-router-dom": "5.2.0",
    "react-tile-map": "^0.3.3",
    "react-toastify": "^8.1.0",
    "react-use-storage": "0.4.3",
    "rewire": "5.0.0",
    "subscriptions-transport-ws": "^0.11.0",
    "swr": "^1.3.0",
    "use-query-params": "^2.1.0",
    "web3": "^1.7.0",
    "web3-eth": "^1.7.0",
    "web3-eth-contract": "^1.7.0",
